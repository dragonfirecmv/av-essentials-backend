    "@nestjs/core": "^6.0.0",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.5.3",
    "@nestjs/typeorm": "^6.1.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "get-random-values": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.12",
